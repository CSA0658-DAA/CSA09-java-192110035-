package exam;
import java.util.*;
public class pattren {
	public static void main(String[] args) {
		Scanner s=new Scanner(System.in);
		System.out.print("Enetr the number : ");
		int n=s.nextInt();
		for (int j=1;j<=n;j++)
		{ for (int k=n;k>j;k--)
		  {System.out.print(" ");}
		   for (int i=1;i<=n;i++)
		   {  System.out.print("*");
		   }
		  System.out.println();
	
		}
	}
}
